    },
    "type": "module",
    "devDependencies": {
        "@vite-pwa/assets-generator": "^0.2.6",
        "ava": "^6.2.0",
        "fudgel": "^1.5.4",
        "node-stream-zip": "^1.15.0",
        "rollup-plugin-minify-html-literals-v3": "^1.3.4",
        "rxjs": "^7.8.1",
        "typescript": "^5.7.3",
        "vite": "^6.0.11",
        "vite-plugin-pwa": "^0.21.1",
        "vite-plugin-simple-html": "^0.2.0",
        "vite-plugin-version-mark": "^0.1.4"
    },
    "dependencies": {
        "@bencevans/kalman-filter": "^1.0.0",
        "@tofandel/qrcode-svg": "^1.2.0",
        "@types/ecef-projector": "^1.0.2",
        "@types/suncalc": "^1.9.2",
        "@types/w3c-generic-sensor": "^1.0.10",
        "@undecaf/zbar-wasm": "^0.11.0",
        "cheap-ruler": "^4.0.0",
        "ecef-projector": "^1.0.1",
        "js-base64": "^3.7.7",
        "luby-transform": "^0.2.0",
        "suncalc": "^1.9.0",
        "tsx": "^4.19.2",
        "usng.js": "^0.4.5"
    },
    "ava": {
        "extensions": {
            "cjs": true,
            "mjs": true,
            "js": true,
            "ts": "commonjs"
        },
        "files": [
            "src/**/*.test.ts"
        ]
    }
